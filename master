# fibonacci-series
//9th program on c
#include<stdio.h>
int main()
{
int t1=0,t2=1,new=0,i,n;
printf("Enter the number of terms \n");
scanf("%d",&n);
printf("The fibonacci series is \n");
printf("%d, ",t1);
printf("%d, ",t2); 
for(i=1;i<=n;i++)
{
new=t1+t2;
t1=t2;
t2=new;
printf("%d, ",new);
}
return(0);
}
